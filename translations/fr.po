# Label: any.custom
#: any.custom
msgid "{{#label}} failed custom validation because {{#error.message}}"
msgstr ""
"La validation personnalisée de {{#label}} a échoué car {{#error.message}}"

# Label: any.default
#: any.default
msgid "{{#label}} threw an error when running default method"
msgstr ""
"Une erreur s'est produite lors de l'exécution de la méthode par défaut "
"{{#label}}"

# Label: any.failover
#: any.failover
msgid "{{#label}} threw an error when running failover method"
msgstr ""
"{Une erreur s'est produite lors de l'exécution de la méthode de basculement"

# Label: any.invalid
#: any.invalid
msgid "{{#label}} contains an invalid value"
msgstr "{{#label}} contient une valeur non valide"

# Label: any.only
#: any.only
msgid ""
"{{#label}} must be {if(#valids.length == 1, \"\", \"one of \")}{{#valids}}"
msgstr ""
"{{#label}} doit être {if(#valids.length == 1, \"\", \"one of \")}{{#valids}}"

# Label: any.ref
#: any.ref
msgid "{{#label}} {{#arg}} references {{:#ref}} which {{#reason}}"
msgstr "{{#label}} {{#arg}} les références {{:#ref}} qui {{#reason}}"

# Label: any.required
#: any.required
msgid "{{#label}} is required"
msgstr "{{#label}} est nécessaire"

# Label: object.unknown
#: object.unknown
msgid "{{#label}} is not allowed"
msgstr "{{#label}} n'est pas autorisé"

# Label: alternatives.all
#: alternatives.all
msgid "{{#label}} does not match all of the required types"
msgstr "{{#label}} ne correspond pas à tous les types requis"

# Label: array.includes
#: array.includes
msgid "{{#label}} does not match any of the allowed types"
msgstr "{{#label}} ne correspond à aucun des types autorisés"

# Label: alternatives.one
#: alternatives.one
msgid "{{#label}} matches more than one allowed type"
msgstr "{{#label}} correspond à plus d'un type autorisé"

# Label: alternatives.types
#: alternatives.types
msgid "{{#label}} must be one of {{#types}}"
msgstr "{{#label}} doit être l'un des {{#types}}"

# Label: array.base
#: array.base
msgid "{{#label}} must be an array"
msgstr "{{#label}} doit être un tableau"

# Label: array.excludes
#: array.excludes
msgid "{{#label}} contains an excluded value"
msgstr "{{#label}} contient une valeur exclue"

# Label: array.hasKnown
#: array.hasKnown
msgid ""
"{{#label}} does not contain at least one required match for type {:"
"#patternLabel}"
msgstr ""
"{{#label}} ne contient pas au moins une correspondance requise pour le type "
"{:#patternLabel}"

# Label: array.hasUnknown
#: array.hasUnknown
msgid "{{#label}} does not contain at least one required match"
msgstr "{{#label}} ne contient pas au moins une correspondance requise"

# Label: array.includesRequiredBoth
#: array.includesRequiredBoth
msgid ""
"{{#label}} does not contain {{#knownMisses}} and {{#unknownMisses}} other "
"required value(s)"
msgstr ""
"{{#label}} ne contient pas {{#knownMisses}} et {{#unknownMisses}} d'autre(s) "
"valeur(s) requise(s)"

# Label: array.includesRequiredKnowns
#: array.includesRequiredKnowns
msgid "{{#label}} does not contain {{#knownMisses}}"
msgstr "{{#label}} ne contient pas {{#knownMisses}}"

# Label: array.includesRequiredUnknowns
#: array.includesRequiredUnknowns
msgid "{{#label}} does not contain {{#unknownMisses}} required value(s)"
msgstr "{{#label}} ne contient pas de {{#unknownMisses}} valeur(s) requise(s)"

# Label: array.length
#: array.length
msgid "{{#label}} must contain {{#limit}} items"
msgstr "{{#label}} doit contenir des éléments {{#limit}}"

# Label: array.max
#: array.max
msgid "{{#label}} must contain less than or equal to {{#limit}} items"
msgstr ""
"{{#label}} doit contenir un nombre d'éléments inférieur ou égal à {{#limit}}"

# Label: array.min
#: array.min
msgid "{{#label}} must contain at least {{#limit}} items"
msgstr "{{#label}} doit contenir au moins des éléments {{#limit}}"

# Label: array.orderedLength
#: array.orderedLength
msgid "{{#label}} must contain at most {{#limit}} items"
msgstr "{{#label}} doit contenir au plus des éléments {{#limit}}"

# Label: array.sort
#: array.sort
msgid "{{#label}} must be sorted in {#order} order by {{#by}}"
msgstr "{{#label}} doit être trié dans l'ordre {#order} par {{#by}}"

# Label: array.sort.mismatching
#: array.sort.mismatching
msgid "{{#label}} cannot be sorted due to mismatching types"
msgstr "{{#label}} ne peut être trié en raison de la non-concordance des types"

# Label: array.sort.unsupported
#: array.sort.unsupported
msgid "{{#label}} cannot be sorted due to unsupported type {#type}"
msgstr ""
"{{#label}} ne peut être trié en raison d'un type non pris en charge {#type}"

# Label: array.sparse
#: array.sparse
msgid "{{#label}} must not be a sparse array item"
msgstr "{{#label}} ne doit pas être un élément de tableau épars"

# Label: array.unique
#: array.unique
msgid "{{#label}} contains a duplicate value"
msgstr "{{#label}} contient une valeur en double"

# Label: boolean.base
#: boolean.base
msgid "{{#label}} must be a boolean"
msgstr "{{#label}} doit être un booléen"

# Label: date.base
#: date.base
msgid "{{#label}} must be a valid date"
msgstr "{{#label}} doit être une date valide"

# Label: date.format
#: date.format
msgid ""
"{{#label}} must be in {msg(\"date.format.\" + #format) || #format} format"
msgstr ""
"{{#label}} doit être dans le format {msg(\"date.format.\" + #format) || "
"#format}"

# Label: date.greater
#: date.greater
msgid "{{#label}} must be greater than {{:#limit}}"
msgstr "{{#label}} doit être supérieur à {{:#limit}}"

# Label: date.less
#: date.less
msgid "{{#label}} must be less than {{:#limit}}"
msgstr "{{#label}} doit être inférieur à {{:#limit}}"

# Label: date.max
#: date.max
msgid "{{#label}} must be less than or equal to {{:#limit}}"
msgstr "{{#label}} doit être inférieur ou égal à {{:#limit}}"

# Label: date.min
#: date.min
msgid "{{#label}} must be greater than or equal to {{:#limit}}"
msgstr "{{#label}} doit être supérieur ou égal à {{:#limit}}"

# Label: date.format.iso
#: date.format.iso
msgid "ISO 8601 date"
msgstr "Date ISO 8601"

# Label: date.format.javascript
#: date.format.javascript
msgid "timestamp or number of milliseconds"
msgstr "Horodatage ou nombre de millisecondes"

# Label: date.format.unix
#: date.format.unix
msgid "timestamp or number of seconds"
msgstr "Horodatage ou nombre de secondes"

# Label: object.and
#: object.and
msgid ""
"{{#label}} contains {{#presentWithLabels}} without its required peers "
"{{#missingWithLabels}}"
msgstr ""
"{{#label}} contient {{#presentWithLabels}} sans ses pairs "
"{{#missingWithLabels}}"

# Label: object.assert
#: object.assert
msgid ""
"{{#label}} is invalid because {if(#subject.key, `\"` + #subject.key + `\" "
"failed to ` + (#message || \"pass the assertion test\"), #message || \"the "
"assertion failed\")}"
msgstr ""
"{{#label}} est invalide car {if(#subject.key, `\"` + #subject.key + `\" "
"failed to ` + (#message || \"pass the assertion test\"), #message || \"the "
"assertion failed\")}"

# Label: object.base
#: object.base
msgid "{{#label}} must be of type {{#type}}"
msgstr "{{#label}} doit être du type {{#type}}"

# Label: object.instance
#: object.instance
msgid "{{#label}} must be an instance of {{:#type}}"
msgstr "{{#label}} doit être une instance de {{:#type}}"

# Label: object.length
#: object.length
msgid "{{#label}} must have {{#limit}} key{if(#limit == 1, \"\", \"s\")}"
msgstr "{{#label}} doit avoir {{#limit}} clé{if(#limit == 1, \"\", \"s\")}"

# Label: object.max
#: object.max
msgid ""
"{{#label}} must have less than or equal to {{#limit}} key{if(#limit == 1, "
"\"\", \"s\")}"
msgstr ""
"{{#label}} doit être inférieur ou égal à {{#limit}} clé{if(#limit == 1, "
"\"\", \"s\")}"

# Label: object.min
#: object.min
msgid ""
"{{#label}} must have at least {{#limit}} key{if(#limit == 1, \"\", \"s\")}"
msgstr ""
"{{#label}} doit avoir au moins {{#limit}} clé{if(#limit == 1, \"\", \"s\")}"

# Label: object.missing
#: object.missing
msgid "{{#label}} must contain at least one of {{#peersWithLabels}}"
msgstr "{{#label}} doit contenir au moins un {{#peersWithLabels}}"

# Label: object.nand
#: object.nand
msgid ""
"{{:#mainWithLabel}} must not exist simultaneously with {{#peersWithLabels}}"
msgstr ""
"{{:#mainWithLabel}} ne doit pas exister en même temps que "
"{{#peersWithLabels}}"

# Label: object.oxor
#: object.oxor
msgid ""
"{{#label}} contains a conflict between optional exclusive peers "
"{{#peersWithLabels}}"
msgstr ""
"{{#label}} contient un conflit entre des pairs exclusifs facultatifs "
"{{#peersWithLabels}}"

# Label: object.pattern.match
#: object.pattern.match
msgid "{{#label}} keys failed to match pattern requirements"
msgstr "Les clés {{#label}} ne correspondent pas aux exigences du modèle"

# Label: object.refType
#: object.refType
msgid "{{#label}} must be a Joi reference"
msgstr "{{#label}} doit être une référence Joi"

# Label: object.regex
#: object.regex
msgid "{{#label}} must be a RegExp object"
msgstr "{{#label}} doit être un objet RegExp"

# Label: object.rename.multiple
#: object.rename.multiple
msgid ""
"{{#label}} cannot rename {{:#from}} because multiple renames are disabled "
"and another key was already renamed to {{:#to}}"
msgstr ""
"{{#label}} ne peut pas renommer {{:#from}} parce que les renommages "
"multiples sont désactivés et qu'une autre clé a déjà été renommée en {{:#to}}"

# Label: object.rename.override
#: object.rename.override
msgid ""
"{{#label}} cannot rename {{:#from}} because override is disabled and target "
"{{:#to}} exists"
msgstr ""
"{{#label}} ne peut pas renommer {{:#from}} car l'annulation est désactivée "
"et la cible {{:#to}} existe"

# Label: object.schema
#: object.schema
msgid "{{#label}} must be a Joi schema of {{#type}} type"
msgstr "{{#label}} doit être un schéma Joi de type {{#type}}"

# Label: object.with
#: object.with
msgid "{{:#mainWithLabel}} missing required peer {{:#peerWithLabel}}"
msgstr ""
"{{:#mainWithLabel}} missing required peer {{:#peerWithLabel}} (pair manquant)"

# Label: object.without
#: object.without
msgid "{{:#mainWithLabel}} conflict with forbidden peer {{:#peerWithLabel}}"
msgstr "{{:#mainWithLabel}} conflit avec un pair interdit {{:#peerWithLabel}}"

# Label: object.xor
#: object.xor
msgid ""
"{{#label}} contains a conflict between exclusive peers {{#peersWithLabels}}"
msgstr ""
"{{#label}} contient un conflit entre des pairs exclusifs {{#peersWithLabels}}"

# Label: function.arity
#: function.arity
msgid "{{#label}} must have an arity of {{#n}}"
msgstr "{{#label}} doit avoir un arité de {{#n}}"

# Label: function.class
#: function.class
msgid "{{#label}} must be a class"
msgstr "{{#label}} doit être une classe"

# Label: function.maxArity
#: function.maxArity
msgid "{{#label}} must have an arity lesser or equal to {{#n}}"
msgstr "{{#label}} doit avoir une arité inférieure ou égale à {{#n}}"

# Label: function.minArity
#: function.minArity
msgid "{{#label}} must have an arity greater or equal to {{#n}}"
msgstr "{{#label}} doit avoir un arité supérieur ou égal à {{#n}}"

# Label: number.base
#: number.base
msgid "{{#label}} must be a number"
msgstr "{{#label}} doit être un nombre"

# Label: number.greater
#: number.greater
msgid "{{#label}} must be greater than {{#limit}}"
msgstr "{{#label}} doit être supérieur à {{#limit}}"

# Label: number.infinity
#: number.infinity
msgid "{{#label}} cannot be infinity"
msgstr "{{#label}} ne peut pas être l'infini"

# Label: number.integer
#: number.integer
msgid "{{#label}} must be an integer"
msgstr "{{#label}} doit être un nombre entier"

# Label: number.less
#: number.less
msgid "{{#label}} must be less than {{#limit}}"
msgstr "{{#label}} doit être inférieur à {{#limit}}"

# Label: number.max
#: number.max
msgid "{{#label}} must be less than or equal to {{#limit}}"
msgstr "{{#label}} doit être inférieur ou égal à {{#limit}}"

# Label: number.min
#: number.min
msgid "{{#label}} must be greater than or equal to {{#limit}}"
msgstr "{{#label}} doit être supérieur ou égal à {{#limit}}"

# Label: number.multiple
#: number.multiple
msgid "{{#label}} must be a multiple of {{#multiple}}"
msgstr "{{#label}} doit être un multiple de {{#multiple}}"

# Label: number.negative
#: number.negative
msgid "{{#label}} must be a negative number"
msgstr "{{#label}} doit être un nombre négatif"

# Label: number.port
#: number.port
msgid "{{#label}} must be a valid port"
msgstr "{{#label}} doit être un port valide"

# Label: number.positive
#: number.positive
msgid "{{#label}} must be a positive number"
msgstr "{{#label}} doit être un nombre positif"

# Label: number.precision
#: number.precision
msgid "{{#label}} must have no more than {{#limit}} decimal places"
msgstr "{{#label}} ne doit pas avoir plus de {{#limit}} décimales"

# Label: number.unsafe
#: number.unsafe
msgid "{{#label}} must be a safe number"
msgstr "{{#label}} doit être un nombre sûr"

# Label: string.alphanum
#: string.alphanum
msgid "{{#label}} must only contain alpha-numeric characters"
msgstr "{{#label}} ne doit contenir que des caractères alphanumériques"

# Label: string.base
#: string.base
msgid "{{#label}} must be a string"
msgstr "{{#label}} doit être une chaîne de caractères"

# Label: string.base64
#: string.base64
msgid "{{#label}} must be a valid base64 string"
msgstr "{{#label}} doit être une chaîne base64 valide"

# Label: string.creditCard
#: string.creditCard
msgid "{{#label}} must be a credit card"
msgstr "{{#label}} doit être une carte de crédit"

# Label: string.dataUri
#: string.dataUri
msgid "{{#label}} must be a valid dataUri string"
msgstr "{{#label}} doit être une chaîne dataUri valide"

# Label: string.domain
#: string.domain
msgid "{{#label}} must contain a valid domain name"
msgstr "{{#label}} doit contenir un nom de domaine valide"

# Label: string.email
#: string.email
msgid "{{#label}} must be a valid email"
msgstr "{{#label}} doit être un email valide"

# Label: string.empty
#: string.empty
msgid "{{#label}} is not allowed to be empty"
msgstr "{{#label}} ne peut être vide"

# Label: string.guid
#: string.guid
msgid "{{#label}} must be a valid GUID"
msgstr "{{#label}} doit être un GUID valide"

# Label: string.hex
#: string.hex
msgid "{{#label}} must only contain hexadecimal characters"
msgstr "{{#label}} ne doit contenir que des caractères hexadécimaux"

# Label: string.hexAlign
#: string.hexAlign
msgid "{{#label}} hex decoded representation must be byte aligned"
msgstr ""
"{{#label}} la représentation décodée en hexadécimal doit être alignée sur "
"l'octet"

# Label: string.hostname
#: string.hostname
msgid "{{#label}} must be a valid hostname"
msgstr "{{#label}} doit être un nom d'hôte valide"

# Label: string.ip
#: string.ip
msgid "{{#label}} must be a valid ip address with a {{#cidr}} CIDR"
msgstr "{{#label}} doit être une adresse IP valide avec un {{#cidr}} CIDR"

# Label: string.ipVersion
#: string.ipVersion
msgid ""
"{{#label}} must be a valid ip address of one of the following versions "
"{{#version}} with a {{#cidr}} CIDR"
msgstr ""
"{{#label}} doit être une adresse IP valide de l'une des versions suivantes "
"{{#version}} avec un {{#cidr}} CIDR"

# Label: string.isoDate
#: string.isoDate
msgid "{{#label}} must be in iso format"
msgstr "{{#label}} doit être au format iso"

# Label: string.isoDuration
#: string.isoDuration
msgid "{{#label}} must be a valid ISO 8601 duration"
msgstr "{{#label}} doit être une durée ISO 8601 valide"

# Label: string.length
#: string.length
msgid "{{#label}} length must be {{#limit}} characters long"
msgstr "La longueur de {{#label}} doit être de {{#limit}} caractères"

# Label: string.lowercase
#: string.lowercase
msgid "{{#label}} must only contain lowercase characters"
msgstr "{{#label}} ne doit contenir que des caractères minuscules"

# Label: string.max
#: string.max
msgid ""
"{{#label}} length must be less than or equal to {{#limit}} characters long"
msgstr ""
"La longueur de {{#label}} doit être inférieure ou égale à {{#limit}} "
"caractères"

# Label: string.min
#: string.min
msgid "{{#label}} length must be at least {{#limit}} characters long"
msgstr "La longueur de {{#label}} doit être d'au moins {{#limit}} caractères"

# Label: string.normalize
#: string.normalize
msgid "{{#label}} must be unicode normalized in the {{#form}} form"
msgstr "{{#label}} doit être normalisé en unicode sous la forme {{#form}}"

# Label: string.token
#: string.token
msgid "{{#label}} must only contain alpha-numeric and underscore characters"
msgstr ""
"{{#label}} ne doit contenir que des caractères alphanumériques et des traits "
"de soulignement"

# Label: string.pattern.base
#: string.pattern.base
msgid ""
"{{#label}} with value {:[.]} fails to match the required pattern: {{#regex}}"
msgstr ""
"{{#label}} avec la valeur {:[.]} ne correspond pas au modèle requis : "
"{{#regex}}"

# Label: string.pattern.name
#: string.pattern.name
msgid "{{#label}} with value {:[.]} fails to match the {{#name}} pattern"
msgstr "{{#label}} avec la valeur {:[.]} ne correspond pas au motif {{#name}}"

# Label: string.pattern.invert.base
#: string.pattern.invert.base
msgid "{{#label}} with value {:[.]} matches the inverted pattern: {{#regex}}"
msgstr ""
"{{#label}} avec la valeur {:[.]} correspond au modèle inversé : {{#regex}}"

# Label: string.pattern.invert.name
#: string.pattern.invert.name
msgid "{{#label}} with value {:[.]} matches the inverted {{#name}} pattern"
msgstr ""
"{{#label}} avec la valeur {:[.]} correspond au modèle inversé {{#name}}"

# Label: string.trim
#: string.trim
msgid "{{#label}} must not have leading or trailing whitespace"
msgstr "{{#label}} ne doit pas comporter d'espace en début ou en fin de phrase"

# Label: string.uri
#: string.uri
msgid "{{#label}} must be a valid uri"
msgstr "{{#label}} doit être un uri valide"

# Label: string.uriCustomScheme
#: string.uriCustomScheme
msgid ""
"{{#label}} must be a valid uri with a scheme matching the {{#scheme}} pattern"
msgstr ""
"{{#label}} doit être un uri valide avec un schéma correspondant au modèle "
"{{#scheme}}"

# Label: string.uriRelativeOnly
#: string.uriRelativeOnly
msgid "{{#label}} must be a valid relative uri"
msgstr "{{#label}} doit être un uri relatif valide"

# Label: string.uppercase
#: string.uppercase
msgid "{{#label}} must only contain uppercase characters"
msgstr "{{#label}} ne doit contenir que des caractères majuscules"

# Label: symbol.base
#: symbol.base
msgid "{{#label}} must be a symbol"
msgstr "{{#label}} doit être un symbole"

# Label: symbol.map
#: symbol.map
msgid "{{#label}} must be one of {{#map}}"
msgstr "{{#label}} doit être l'un des {{#map}}"

# Label: binary.base
#: binary.base
msgid "{{#label}} must be a buffer or a string"
msgstr "{{#label}} doit être un tampon ou une chaîne de caractères"

# Label: binary.length
#: binary.length
msgid "{{#label}} must be {{#limit}} bytes"
msgstr "{{#label}} doit être {{#limit}} octets"

# Label: binary.max
#: binary.max
msgid "{{#label}} must be less than or equal to {{#limit}} bytes"
msgstr "{{#label}} doit être inférieur ou égal à {{#limit}} octets"

# Label: binary.min
#: binary.min
msgid "{{#label}} must be at least {{#limit}} bytes"
msgstr "{{#label}} doit être d'au moins {{#limit}} octets"
